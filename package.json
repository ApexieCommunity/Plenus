{
  "name": "plenus-bot",
  "version": "1.0.0",
  "description": "A classic Discord bot made in TypeScript. Not a competitor of Dyno and MEE6, but something that works and for free.",
  "main": "build/src/index.js",
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node16-linux-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "engines": {
    "node": ">=16.7.0",
    "npm": ">=8.3.0"
  },
  "scripts": {
    "clean": "rm -rf build dist",
    "compile": "tsc",
    "build-all": "npm run compile",
    "prestart": "npm run build-all",
    "start": "node build/src/index.js",
    "pm2": "npm run clean && git pull && npm update && npm start"
  },
  "repository": "https://github.com/ApexieDevelopment/ApexieServices",
  "authors": [
    "ItzLightyHD",
    "Stef58",
    "LukeBTW",
    "Kawi",
    "Abdy"
  ],
  "private": true,
  "dependencies": {
    "ainasepics": "^2.1.7",
    "canvacord": "^5.2.3",
    "chalk": "^4.1.2",
    "dbd-dark-dashboard": "^1.5.21",
    "discord-dashboard": "^2.0.3",
    "discord-together": "^1.3.25",
    "discord-xp": "^1.1.16",
    "discord.js": "^13.4.0",
    "dotenv": "^10.0.0",
    "glob": "^7.2.0",
    "mongoose": "^6.1.3",
    "node-fetch": "^3.1.0",
    "request": "^2.88.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  }
}
