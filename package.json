{
  "name": "plenus-bot",
  "version": "1.0.0",
  "description": "A classic Discord bot made in TypeScript. Not a competitor of Dyno and MEE6, but something that works and for free.",
  "main": "src/index.ts",
  "engines": {
    "node": ">=16.7.0",
    "npm": ">=8.3.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "start": "ts-node --transpile-only ./src/index.ts",
    "heroku": "echo \"Starting Plenus in Heroku mode...\" & npm update && ts-node --transpile-only ./src/index.ts"
  },
  "repository": "https://github.com/ApexieDevelopment/ApexieServices",
  "authors": [
    "ItzLightyHD",
    "Stef58",
    "LukeBTW",
    "Kawi",
    "Abdy"
  ],
  "private": true,
  "devDependencies": {
    "dotenv": "^10.0.0",
    "ts-node-dev": "^2.0.0-0"
  },
  "dependencies": {
    "ainasepics": "^2.1.7",
    "canvacord": "^5.2.3",
    "chalk": "^4.1.2",
    "dbd-capriham-theme": "^1.3.0",
    "discord-dashboard": "^1.5.93",
    "discord-together": "^1.3.25",
    "discord-xp": "^1.1.16",
    "discord.js": "^13.4.0",
    "glob": "^7.2.0",
    "mongoose": "^6.1.3",
    "node-fetch": "^3.1.0",
    "request": "^2.88.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  }
}
